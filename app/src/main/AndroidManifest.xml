<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.application">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".myinfo.LoginActivity" />
        <activity android:name=".myinfo.RegisiterActivity" />
        <activity android:name=".myinfo.FindPswActivity" />
        <activity android:name=".myinfo.ModifyPswActivity" />
        <activity android:name=".myinfo.SettingActivity" />
        <activity android:name=".myinfo.UserInfoActivity" />
        <activity android:name=".myinfo.ChangeUserInfoActivity" />
        <activity android:name=".ADetailActivity"></activity>
    </application>

</manifest>